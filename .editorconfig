root = true

[*]
end_of_line = crlf
insert_final_newline = true
charset = utf-8

[*.java]
indent_style = space
indent_size = 4
ij_any_class_annotation_wrap = split_into_lines
ij_any_field_annotation_wrap = split_into_lines
ij_any_method_annotation_wrap = split_into_lines
ij_any_parameter_annotation_wrap = normal
ij_any_variable_annotation_wrap = split_into_lines
ij_java_class_annotation_wrap = split_into_lines
ij_java_field_annotation_wrap = split_into_lines
ij_java_method_annotation_wrap = split_into_lines
ij_java_parameter_annotation_wrap = normal
ij_java_variable_annotation_wrap = split_into_lines
ij_java_annotation_parameter_wrap = off
ij_java_do_not_wrap_after_single_annotation = false
ij_java_align_multiline_annotation_parameters = true
ij_any_wrap_long_lines = true
ij_java_keep_line_breaks = false
max_line_length = 120
ij_visual_guides = 120
